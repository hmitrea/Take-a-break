{
    "name": "Tabe a <br>",
    "description": "Best extension ever",
    "version": "420.69",
    "manifest_version": 2,
    "permissions": ["alarms", "notifications", "storage"],
    "background": {
      "scripts": [ "background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Code",
      "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "icons": {
      "16": "TakeABreakLogo16.png",
      "32": "TakeABreakLogo32.png",
      "48": "TakeABreakLogo48.png",
      "128": "TakeABreakLogo128.png"
    }
  }
  
  
  
  